; TRANSFER A TO NR21		LDZ	A,(LOFRQWRD)		; READ LOW-FREQUENCY		LD	(NR23),A		; TRANSFER A TO NR23		LDZ	A,(HIFRQWRD)		; READ HF		LD	(NR24),A		; TRANSFER A TO NR24		LDZ	A,(AMPLSAME)		LD	E,A		LDZ	A,(AMPLITUD)		; READ AMPLITUDE		CP	E		jp	Z,@NOINITSN2		LDZ	(AMPLSAME),A		LD	(NR22),A		LDZ	(AMPLITUD),A		LDZ	A,(HIFRQWRD)		OR	#80		LD	(NR24),A@NOINITSN2	ld	a,#fd		call	audioplay		jp	copy_zp_2******************************************************************************									    **	CHANNEL 03 AUDIO-PLAY						    **									    ******************************************************************************audiopl3       	LD	A,(VOICTEST3)  		; CHECK IF CHANNEL IS ON		CP	#01		RET	nz		call	copy_3_zp		;*********************************	;*   SOUND-CHIP UPDATE SECTION	 *	;*********************************		LD	A,#80			; SOUND ON BIT <7>		LD	(NR30),A		; TRANSFER A TO NR30		xor	a			; HARDWARE-LENGTH 00		LD	(NR31),A		; TRANSFER A TO NR31		LDZ	A,(LOFRQWRD)		; READ LOW-FREQUENCY		LD	(NR33),A		; TRANSFER A TO NR33			LDZ	A,(AMPLSAME)		LD	E,A		LDZ	A,(AMPLITUD)		; READ AMPLITUDE		CP	E		jp	Z,@NOINITSN3			LDZ	(AMPLSAME),A		LD	(NR32),A		LDZ	(AMPLITUD),A		LDZ	A,(HIFRQWRD)		OR	#80		JP	@SETNR34@NOINITSN3	LDZ	A,(HIFRQWRD)		; READ HF@SETNR34	LD	(NR34),A		; TRANSFER A TO NR34		ld	a,#fb		call	audioplay		jp	copy_zp_3******************************************************************************									    **	 AUDIO-PLAY						            **									    ******************************************************************************audioplay	ld	(channel_id),a	;*********************************	;*   SOUND-CHIP UPDATE SECTION	 *	;*********************************	;*********************************	;*   SPEED-ROUTINE (TWO BYTES)   *	;*********************************		XOR	A			; SET REALPLAY TO #00		LDZ	(REALPLAY),A		;		LDZ	A,(SPDCOUNT)		; DECREASE SPEEDCOUNT		DEC	A			;		LDZ	(SPDCOUNT),A		;		JP	NZ,@SPDHDX1		; NOT 0, EXIT SPEED-HANDLE		LDZ	A,(SPDCHECK)		; INVERT SPDCHECK		CPL				;		LDZ	(SPDCHECK),A		;		JP	NZ,@SECONS1		; IF SPDCHECK #FF, GOTO 2ND-SPD		LDZ	A,(AUDIONUM)		; READ SPEED 01		LD	C,A		LD	B,#00			;		LD	HL,AUDIOSPDA		;		ADD	HL,BC			;		LD	A,(HL)			;		JP	@SPDSET1		; JUMP TO SPDSET@SECONS1	LDZ	A,(AUDIONUM)		; READ SPEED 02		LD	C,A		LD	B,#00			;		LD	HL,AUDIOSPDB		;		ADD	HL,BC			;		LD	A,(HL)			;@SPDSET1	LDZ	(SPDCOUNT),A		; SET A TO SPDCOUNT		LD	A,#FF			; SET REALPLAY TO #FF		LDZ	(REALPLAY),A		;@SPDHDX1		LDZ	A,(REALPLAY) 		; LENGTH SUBTRACTION NECESSARY?		OR	A			;		JP	Z,@NOSTPH1		; NO, GOTO... @AUDIPL1  	LDZ	A,(LENGCOPY) 		; DECREASE LENGTH		DEC	A			;		LDZ	(LENGCOPY),A		;		JP	Z,@STPHDL1		; IF 0, THEN GOTO STEPHANDLE@NOSTPH1	LDZ	A,(REALNOTE) 		; IF REALNOTE PAUSED, NO FX 		CP	#FF				CALL	NZ,@FXPLAY1		@NOFXPL1	XOR	A			; CLEAR NOTEINIT		LDZ	(NOTEINIT),A 		; 			RET				; 	;*********************************	;*   STEP-HANDLE		 *	;*********************************@STPHDL1		XOR	A			; GLIDE-RESET (!)		LDZ	(GLDSPEED),A 			LDZ	A,(STPINDLO)		; TRANSFER STEPINDEX TO HL		LD	L,A		LDZ	A,(STPINDHI)		LD	H,A		LD	A,(HLI)		CP	#FF			; STEP-END-CHECK 		jp	Z,@SEQHDL1		; YO, GOTO SEQUENCE-HANDLE		JP	@STPHD21 		; NO, GOTO STEPHANDLE 02	;*********************************	;*   SEQUENCE-HANDLE		 *	;*********************************@SEQHDL1		LDZ	A,(SEQINDLO)		; TAKE SEQUENCE-INDEX		LD	L,A		LDZ	A,(SEQINDHI)		LD	H,A				LD	A,(HLI)		CP	#FF			; SEQ-END-REPEAT-CHECK		jp	NZ,@SEQHD21		LD	E,(HL)			; TAKE SEQ-REPEAT POINT		LDZ	A,(AUDIONUM)		; RESET	SEQINDEX WITH		LD	C,A			; SEQREPEAT OFFSET		LD	B,#00	    		LD	HL,AUDIOTAB 		ADD	HL,BC	    		LD	A,(HLI)			; READ 'RESTART' SEQINDEX		LDZ	(SEQINDLO),A		LD	A,(HL)		LDZ	(SEQINDHI),A			LDZ	A,(SEQINDLO)		; TRANSFER SEQ-INDEX TO HL		LD	L,A		LDZ	A,(SEQINDHI)		LD	H,A		LD	C,E	    		ADD	HL,BC	    		LD	A,(HLI)			; FIRST SEQUENCE TO REPEAT		JP	@SEQHD51		; GOTO NEXT SEQUENCE		@SEQHD21	CP	#FE			; SEQ-STOP-CHECK		jp	NZ,@SEQHD31		;		xor	a			; YES, DISABLE VOICE		LDZ	(VOICTEST),A		;		ld	a,(channel_id)		ld	d,a		LD	a,(NR52)	       	and	d		ld	(nr52),a		RET@seqhd31@SEQHD51	CP	#80			; NOTE TRANSPOSE CHECK		jp	C,@SEQHD61    		AND	#7F		LDZ	(TRANBYTE),A				LD	A,(HLI)@SEQHD61		CP	#70			; SFIX-TRANSPOSE CHECK		jp	C,@SEQHD91		AND	#0F		LDZ	(SFXTRANS),A		LD	A,(HLI)@SEQHD91			ADD	A,A		LDZ	(STEPNUMB),A		; SET STEP & CLEAR STEPINDEX		LD	A,L		LDZ	(SEQINDLO),A		LD	A,H		LDZ	(SEQINDHI),A		LDZ	A,(STEPNUMB)		; CALCULATE STEP-INDEX+POSIT		LD	C,A		LD	B,#00		LD	HL,STEPTAB		ADD	HL,BC		LD	A,(HLI)		LDZ	(STPINDLO),A		LD	A,(HL)		LDZ	(STPINDHI),A			JP	@STPHDL1		; BACK TO STEP-HANDLE	;*********************************	;*   STEP-HANDLE 2		 *	;*********************************@STPHD21 	CP	#FD			; GLIDE CHECK IN STEP		jp	NZ,@STPHD41@STPHDG1	LD	A,(HLI)		LDZ	(GLDSPEED),A		LD	A,(HLI)		LD	E,A		LDZ	A,(TRANBYTE)		ADD	A,E		LDZ	(REALNOTE),A		LD	A,(HLI)		LD	E,A		LDZ	A,(TRANBYTE)		ADD	A,E		LDZ	(DESTNOTE),A		JP	@NONEWN1@STPHD41	CP	#E0			; PAUSE CHECK		jp	C,@STPHD51		AND	#1F		LDZ	(REALLENG),A		LD	A,#FF		LDZ	(REALNOTE),A	       	xor	a		LDZ	(AMPLITUD),A		LD	A,L		LDZ	(STPINDLO),A		LD	A,H		LDZ	(STPINDHI),A		JP	@PAUSEX1@STPHD51	CP	#C0			; SOUND CHECK		jp	C,@STPHD61		ld	e,a		ld	a,(channel_id)		cp	#fb		ld	a,e		ld	e,#1f		jp	nz,@stphd52		ld	e,#0f@stphd52	AND	e		LD	E,A		LDZ	A,(SFXTRANS)		ADD	A,E		ADD	A,A		ADD	A,A		LDZ	(REALSOUN),A		LD	A,(HLI)		CP	#FD		jp	Z,@STPHDG1		CP	#C0		jp	C,@STPHD61		DEC	HL		jp	@NONEWN1	@STPHD61	CP	#80			; LENGTH CHECK		jp	C,@STPHD71		AND	#7F		LDZ	(REALLENG),A		LD	A,(HLI)		CP	#FD		jp	Z,@STPHDG1		CP	#80		jp	C,@STPHD71		DEC	HL		jp	@NONEWN1@STPHD71	CP	#60			; ARP CHECK		jp	C,@STPHD81		AND	#1F			LDZ	(ARPNUMBO),A		LD	A,(HLI)		CP	#60		jp	C,@STPHD81		DEC	HL		jp	@NONEWN1@STPHD81			LD	E,A			;SET NOTE + NOTE TRANSPOSE		LDZ	A,(TRANBYTE)		ADD	A,E				LDZ	(REALNOTE),A@NONE
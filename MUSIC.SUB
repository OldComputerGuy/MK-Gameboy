**********************************************									    **	INITIALISATION OF INDIVIDUAL AUDIO-CHANNELS			    **									    ******************************************************************************		; ENTRY : A = SEQUENCE NO.AUDIOIN1	ld	d,a		call	copy_1_zp		ld	e,1		call	audio_init		jp	copy_zp_1		AUDIOIN2	ld	d,a		call	copy_2_zp		ld	e,2		call	audio_init		jp	copy_zp_2		AUDIOIN3	ld	d,a		call	copy_3_zp		ld	e,4		call	audio_init		jp	copy_zp_3		AUDIOIN4	ld	d,a		call	copy_4_zp		ld	e,8		call	audio_init		jp	copy_zp_4		copy_1_zp	ld	hl,VOICTEST1		jp	copy_buff_zpcopy_2_zp	ld	hl,VOICTEST2		jp	copy_buff_zpcopy_3_zp	ld	hl,VOICTEST3		jp	copy_buff_zpcopy_4_zp	ld	hl,VOICTEST4		ld	c,<VOICTEST			;zpbase		call	copy_buff_zp_12		inc	c		call	copy_buff_zp_3		inc	c		inc	c		inc	c		call	copy_buff_zp_3		ld	a,c		add	a,6						ld	c,a		call	copy_buff_zp_3		inc	c		jp	copy_buff_zp_2copy_zp_1	ld	hl,VOICTEST1		jp	copy_zp_buffcopy_zp_2	ld	hl,VOICTEST2		jp	copy_zp_buffcopy_zp_3	ld	hl,VOICTEST3		jp	copy_zp_buffcopy_zp_4	ld	hl,VOICTEST4		ld	c,<VOICTEST		;zpbase		call	copy_zp_buff_12		inc	c		call	copy_zp_buff_3		inc	c		inc	c		inc	c		call	copy_zp_buff_3		ld	a,c		add	a,6						ld	c,a		call	copy_zp_buff_3		inc	c		jp	copy_zp_buff_2copy_zp_buff	ld	c,<VOICTEST			;zpbase		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(hli),a		inc	c		ld	a,(c)		ld	(h